{
    "presets": [
        "env",
        "stage-0",
        "react"
    ],
    "env": {
        "development": {
            "presets": ["react-hmre"]
        }
    },
    "plugins": [
        "transform-decorators-legacy",
        ["import", {
            "libraryName": "antd-mobile",
            "style": true
        }],
        ["transform-runtime", {
            "helpers": false
        }]
    ]
}